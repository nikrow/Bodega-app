{
  "version": "3",
  "server": {
    "command": "php artisan octane:start --server=roadrunner --host=0.0.0.0 --port=${PORT}"
  },
  "http": {
    "address": "0.0.0.0:${PORT}",
    "workers": {
      "max_jobs": 1000,
      "reset_timeout": 3600
    }
  },
  "logs": {
    "level": "error",
    "channels": {
      "http": {
        "level": "error"
      }
    }
  }
}